<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.accolite.hibernate1.assign.Library" table="LIBRARY">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="title" type="string" column="title" />
		<property name="subtitle" type="string" column="subtitle" />
		<property name="author" type="string" column="author" />
		<property name="publisher" type="string" column="publisher" />
		<property name="pages" type="int" column="pages" />
	</class>
</hibernate-mapping>